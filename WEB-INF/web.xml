<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0"
		 metadata-complete="true">
		 
	<servlet>
		<servlet-name>Index</servlet-name>	
		<servlet-class>com.web.IndexServlet</servlet-class>
	</servlet>
        
	<servlet-mapping>
		<servlet-name>Index</servlet-name>  
		<url-pattern>/IndexServlet.do</url-pattern>	
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Certificate</servlet-name>
		<servlet-class>com.web.CertificateServlet</servlet-class>
	</servlet>
        
	<servlet-mapping>
		<servlet-name>Certificate</servlet-name> 
		<url-pattern>/CertificateServlet.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Insert</servlet-name>	
		<servlet-class>com.web.InsertEventServlet</servlet-class>
	</servlet>
        
	<servlet-mapping>
		<servlet-name>Insert</servlet-name>  
		<url-pattern>/InsertEventServlet.do</url-pattern>	
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EventInfo</servlet-name>	
		<servlet-class>com.web.EventInfoServlet</servlet-class>
	</servlet>
        
	<servlet-mapping>
		<servlet-name>EventInfo</servlet-name>  
		<url-pattern>/EventInfoServlet.do</url-pattern>	
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Apply</servlet-name>	
		<servlet-class>com.web.ApplyServlet</servlet-class>
	</servlet>
        
	<servlet-mapping>
		<servlet-name>Apply</servlet-name>  
		<url-pattern>/ApplyServlet.do</url-pattern>	
	</servlet-mapping>
	
	
    <welcome-file-list>
        <welcome-file>IndexServlet.do</welcome-file>
    </welcome-file-list>
 

	<context-param>
		<param-name>EFile</param-name>
		<param-value>Event.json</param-value>
		
		<param-name>AFile</param-name>
		<param-value>Applier.json</param-value>
		
		<param-name>PFile</param-name>
		<param-value>password.json</param-value>
		
	</context-param>

	<listener>
		<listener-class>
			com.web.EventContextListener
		</listener-class>
	</listener>

	

	

 </web-app>
